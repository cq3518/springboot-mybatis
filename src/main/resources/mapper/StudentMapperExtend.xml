<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ws.study.springboot.mybatis.edu.mapper.StudentMapper">

    <select id="selectByUsername" parameterType="java.lang.String" resultType="com.ws.study.springboot.mybatis.edu.entity.Student">
        select * from edu_student where username like #{username}
    </select>

</mapper>
